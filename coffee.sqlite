CREATE TABLE coffee (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  name TEXT NOT NULL,
  roast_level TEXT NOT NULL,
  ground_whole_bean TEXT NOT NULL,
  flavor_description TEXT,
  price REAL NOT NULL,
  volume INTEGER NOT NULL
);

